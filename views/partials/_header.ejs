<div>
  <% if (!useremail) { %>
    <form action="/login" method="GET">
      <input type="submit" value="Sign in">
    </form>&nbsp;
    <form action="/register" method="GET">
      <input type="submit" value="Register">
    </form>
  <% } else { %>
    <%= useremail %>
    <form action="/logout" method="POST">
      <input type="submit" value="Sign out">
    </form>
  <% } %>

</div>